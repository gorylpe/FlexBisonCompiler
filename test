( sito Eratostenesa 
	ROZWIJANIE PETLI FOR
)
VAR
    n a t[5]
BEGIN
    a := 3;
	READ t[a];
    n := 4;
	t[a] := a;
	t[n] := n;
	t[0] := t[a] + t[n];
	IF t[a] > t[n] THEN
		WRITE t[n];
	ENDIF
	IF t[a] >= t[n] THEN
		WRITE t[n];
	ELSE
		WRITE t[a];
	ENDIF

	FOR i FROM t[a] TO t[n] DO
		WRITE i;
	ENDFOR
END
